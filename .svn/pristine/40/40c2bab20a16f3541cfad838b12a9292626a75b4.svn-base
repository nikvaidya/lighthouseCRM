<div class="h-100">
      <div *ngIf = "dKpi" class='twod-kpi h-100 blue {{dKpi.css_class | lowercase}}' (click) = "itemClick($event)">
        <div class="kpi-header">
          {{dKpi.value[0]}}
        </div>
        <div class="kpi-body">
          {{dKpi.value[1]}}
        </div>
        <div >
          <p>{{dKpi.value[2]}}</p>
        </div>
      </div>
    </div>