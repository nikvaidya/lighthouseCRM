<ion-content class="theme-blue">

  <div class="app-key-container">
    <ion-grid class="upper-container">
      <ion-row>
        <ion-col col-sm-8 padding class="primaryTextColor text-center">
          <div class="appkey-logo">
          <img class="lhs-logo" src="assets/imgs/logo-new.png">
        </div>
        </ion-col>
      </ion-row>
    </ion-grid>
    <form name="appkey" class="key-container" (submit)="save(appkey)"> 
      <div class="form-section">
        <ion-grid>
          <ion-row>
            <!-- <ion-col col-1 col-sm-2></ion-col> -->

            <ion-col col-8 col-sm-8>

              <ion-item>
                <ion-icon name="md-key" item-start class="text-orange"></ion-icon>
                <ion-input type="text" placeholder="Enter App-key" name="App-key  " [(ngModel)]="appkey"></ion-input>
              </ion-item>

              <div class="btn-outer">
                  <ion-button icon-only shape="round"  type="submit" color="primary" class="global-btn" round>
                    Enter</ion-button>
                </div>
            </ion-col>
          </ion-row>
        </ion-grid>
      </div>
    </form>
  </div>
</ion-content>